					  <h2>Danh sách đơn hàng</h2>
					    <table id="table">
						<thead>
						  <tr>
							<th>Mã đơn hàng</th>
							<th>Tên khách hàng</th>
							<th>Địa chỉ</th>
							<th>Số điện thoại</th>
							<th>Ngày lập</th>
							<th>Tình trạng</th>
							<th>Chỉnh sửa</th>
						  </tr>
						</thead>
						<tbody>


								<% danhsach.danhsachsv.forEach(function (motphantu){    			 %>
								<%
								var classStt = "success";
								if(motphantu.Status === "Đang giao"){
									classStt = "info";
								}
								if(motphantu.Status === "Chưa giao"){
									classStt = "danger";
								}

								%>
							<tr class="<%= classStt%>">
							<td><%= motphantu.billID %></td>
							<td><%= motphantu.TenKH %></td>
							<td><%= motphantu.Addr %></td>
							<td><%= motphantu.SDT %></td>

							<%
										var str = motphantu.Date;
										var temp = str.toString().slice(4, 15);
										var date = new Date(temp);
										console.log("===ngay nhap sua3===" + date);
											var month = date.getMonth()+1;
											var thang="";
											thang = month;
											var ngayNhap = date.getDate().toString()+"/" + thang +"/"+ date.getFullYear().toString() ; 
							%>
							<td><%= ngayNhap %></td>
							<td><%= motphantu.Status %></td>
						  <td style="text-align: center;">
							<span >
								<a class="agile-icon" href="/suadonhang/<%= motphantu.billID %>" ><i class="fa fa-pencil-square-o"></i></a>
							</span> 
							<span>
								<a class="agile-icon" onclick="return xoahaykhong('Bạn có chắc chắn xoá không?') ; " href="/xoadonhang/<%= motphantu.billID  %>" ><i class="fa fa-times-circle"></i></a>
							</span>
							  <script type="text/javascript"> 
                              function xoahaykhong(tinnhan){
                                  if(window.confirm(tinnhan)==true){
                                      return true; 
                                  }
                                  return false ; 
                              }

      			 </script>
							</td>
						  </tr>

               <% }) %>
						</tbody>
